{
  "name": "zilwatch",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "start": "npm run uglifyjs && npm run uglifycss && node ./bin/www",
    "test": "nyc mocha --parallel tests/**/*.js",
    "uglifyjs": "uglifyjs clientjs/*.js --compress --mangle --output public/javascripts/main.min.js",
    "uglifycss": "uglifycss clientcss/*.css --ugly-comments --output public/stylesheets/main.min.css"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "~3.0.2",
    "redis": "^3.1.2",
    "uglify-js": "^3.13.4",
    "uglifycss": "^0.0.29",
    "universal-analytics": "^0.4.23"
  },
  "devDependencies": {
    "jquery": "^3.6.0",
    "jsdom": "^16.5.3",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "sinon": "^10.0.0"
  }
}
